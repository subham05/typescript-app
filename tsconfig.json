{
  "compilerOptions": {
  
    "target": "esnext" ,
    "module": "esnext" ,
    "lib": [
      "es2017"
    ],
    "allowJs": true ,
   
    "jsx": "react-native",
    
    "noEmit": true,
   
    "isolatedModules": true ,

  
    "strict": true ,
   
    "moduleResolution": "node" ,
    "baseUrl": "./" ,
    "paths": {
      "tests": ["tests/*"],
      "@common/*": ["./src/common/*"],
      "@screens/*": ["./src/screens/*"],
      "@router/*": ["./src/router/*"],
      "@components/*": ["./src/common/components*"],
      "@theme/*": ["./src/common/theme*"],
      "@style/*": ["./src/common/style*"]
    } ,
  
    "typeRoots": [
      "./types"
    ],
    
    "allowSyntheticDefaultImports": true ,
    "esModuleInterop": true ,
  
    "skipLibCheck": false,
    "resolveJsonModule": true 

    
  },
  "extends": "@react-native/typescript-config/tsconfig.json",
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
